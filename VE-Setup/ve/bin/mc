#!/usr/bin/env ruby

require_relative '../lib/master_control'


mc = RelationshipOfCommand::MasterControl.new
choice, input, output = ARGV[0], ARGV[1], ARGV[2]

case choice
# when 'run_jobs'
  # mc.run_jobs
when 'convert'
  mc.create_job input, output
  mc.run_jobs
else
  puts 'Don\'t know that one'
  puts 'Try: '
  puts 'bin/mc convert testsrc.mpg testsrc.mov'
end
